GameTaskElement {
    background-color: rgba(0, 0, 0, 0.65);
    border-radius: 16px;
    color: rgb(214, 214, 214);
    margin: 10px 0;
    padding: 10px;
}

GameTaskElement Label {
    margin: 0;
    padding: 0;
}

GameTaskElement #name {
    color: rgb(0, 255, 121);
    font-size: 32px;
}

GameTaskElement #description {
    font-size: 24px;
}
